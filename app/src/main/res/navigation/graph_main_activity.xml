<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/graph_main_activity"
  app:startDestination="@id/mainFragment">

  <fragment
    android:id="@+id/mainFragment"
    android:name="com.ys.navigationui.MainFragment"
    android:label="MainFragment">

    <action
      android:id="@+id/action_mainFragment_to_menuActivity"
      app:destination="@id/menuActivity"/>

    <action
      android:id="@+id/action_mainFragment_to_drawerActivity"
      app:destination="@id/drawerActivity"/>

    <action
      android:id="@+id/action_mainFragment_to_bottomNavBarActivity"
      app:destination="@id/bottomNavBarActivity"/>

    <action
      android:id="@+id/action_mainFragment_to_coordinatorLayoutActivity"
      app:destination="@id/coordinatorLayoutActivity"/>
  </fragment>

  <activity
    android:id="@+id/menuActivity"
    android:name="com.ys.navigationui.menu.MenuActivity"
    android:label="menuActivity"/>

  <activity
    android:id="@+id/drawerActivity"
    android:name="com.ys.navigationui.drawerlayout.DrawerLayoutActivity"
    android:label="drawerActivity"/>

  <activity
    android:id="@+id/bottomNavBarActivity"
    android:name="com.ys.navigationui.bottomnavbar.BottomNavBarActivity"
    android:label="bottomNavBarActivity"/>

  <activity
    android:id="@+id/coordinatorLayoutActivity"
    android:name="com.ys.navigationui.coordinatorlayout.CoordinatorLayoutActivity"
    android:label="coordinatorLayoutActivity"/>

</navigation>